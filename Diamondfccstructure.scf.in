&CONTROL
   calculation = 'scf'
   restart_mode='from_scratch',
   prefix='D2',
   tstress = .true.
   tprnfor = .true.
   pseudo_dir = './',
   outdir='./'
/

&SYSTEM
   ibrav = 2                  ! Bravais Lattice index -- 2 for fcc
   celldm(1) = 6.66405         !3.57Lattice parameter in Ang
   nat = 2                    ! Num of atoms in cell
   ntyp = 1 
   nspin=2                  ! Num of atom types
   ecutwfc = 130             ! Cutoff energy in Rydberg
/

&ELECTRONS
   conv_thr = 1.0d-8 ! SCF convergence
   mixing_beta = 0.7 ! mixing factor for self-consistency
/
&ions
 /
 &cell
  press=70.,
 /
ATOMIC_SPECIES
 C 12.011 C.pz-vbc.UPF      !LDA
ATOMIC_POSITIONS crystal
 C 0.000 0.000 0.000
 C 0.250 0.250 0.250
K_POINTS automatic
 8 8 8 0 0 0

